@model prensaestudiantil.Web.Data.Entities.YoutubeVideo

<div class="form-group">
    <label asp-for="URL" class="control-label"></label>
    <span class='glyphicon glyphicon-info-sign title' title="Ayuda" id="modal-help"></span>
    <input asp-for="URL" class="form-control" />
    <span asp-validation-for="URL" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Name" class="control-label"></label>
    <input asp-for="Name" class="form-control" />
    <span asp-validation-for="Name" class="text-danger"></span>
</div>

<div class="form-group" style="text-align: center">
    <a class="btn btn-danger" asp-action="Index">Cancel</a>
    <input type="submit" value="Create" class="btn btn-default btn-success" />
</div>